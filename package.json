{
  "name": "machinepack-sockets",
  "version": "1.2.1",
  "description": "Publish messages to rooms and/or sockets and more using Sails & Socket.io.",
  "scripts": {
    "test": "mocha tests/*.test.js -t 10000 -R spec"
  },
  "keywords": [
    "Sockets",
    "machines",
    "machinepack"
  ],
  "author": "The Treeline Company",
  "license": "MIT",
  "dependencies": {
    "@sailshq/lodash": "3.10.2",
    "machine": "^13.0.0-0",
    "sails.io.js": "^1.1.4",
    "socket.io-client": "1.7.1",
    "machinepack-util": "^7.0.0-2"
  },
  "devDependencies": {
    "mocha": "3.0.2",
    "sails": "^1.0.0-16",
    "sails-hook-sockets": "^1.3.0",
    "test-machinepack-mocha": "^2.1.1"
  },
  "machinepack": {
    "friendlyName": "Socket.io",
    "machineDir": "machines/",
    "machines": [
